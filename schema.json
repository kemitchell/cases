{
  "type": "object",
  "properties": {
    "plaintiffs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "defendants": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "reports": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "number": {
      "type": "string"
    },
    "year": {
      "type": "string",
      "pattern": "^[12][0-9][0-9][0-9]$"
    },
    "court": {
      "enum": [
        "Northern District of California",
        "Northern District of Illinois",
        "District of Columbia",
        "1st Circuit",
        "2nd Circuit",
        "3rd Circuit",
        "4th Circuit",
        "5th Circuit",
        "6th Circuit",
        "7th Circuit",
        "8th Circuit",
        "9th Circuit",
        "10th Circuit",
        "11th Circuit",
        "Federal Circuit",
        "Supreme Court"
      ]
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "claims": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "quotes": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "holdings": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "documents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "digest": {
            "type": "string",
            "pattern": "^[a-f0-9]{64}$"
          },
          "date": {
            "type": "string",
            "format": "date"
          },
          "label": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "digest"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "plaintiffs",
    "defendants",
    "court"
  ],
  "additionalProperties": false
}
